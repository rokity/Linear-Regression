<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.regressionelineare.riccardoamadio.regressionelineare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/combo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/combo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ManualeActivity"
            android:label="Manuale" />
        <activity
            android:name=".RandomActivity"
            android:label="Random" />
        <activity
            android:name=".RisultatiActivity"
            android:label="@string/title_activity_risultati"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>